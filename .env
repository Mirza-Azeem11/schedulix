# we keep sensitve information user name password like as keys 
# Environment Configuration for Schedulix Healthcare Management System

# API Configuration
REACT_APP_API_URL=http://localhost:3000/api
REACT_APP_API_TIMEOUT=10000

# Authentication
REACT_APP_TOKEN_STORAGE_KEY=token
REACT_APP_USER_STORAGE_KEY=user

# Application Configuration
REACT_APP_APP_NAME=Schedulix Healthcare Management
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_REAL_TIME_UPDATES=true

# UI Configuration
REACT_APP_ITEMS_PER_PAGE=10
REACT_APP_NOTIFICATION_TIMEOUT=5000

# Development
NODE_ENV=development
REACT_APP_DEBUG=true